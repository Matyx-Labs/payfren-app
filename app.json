{
  "expo": {
    "owner": "payfren",
    "name": "Payfren",
    "slug": "app",
    "icon": "./assets/icon.png",
    "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#FCFCFC"
    },
    "android": {
      "package": "com.payfren.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptiveIcon.png",
        "backgroundColor": "#FCFCFC"
      }
    },
    "ios": {
      "bundleIdentifier": "com.payfren.app"
    },
    "scheme": "payfren",
    "assetBundlePatterns": [
      "assets/**"
    ],
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "jsEngine": "hermes",
    "plugins": [
      "expo-dev-client"
    ],
    "extra": {
      "eas": {
        "projectId": "4a19a1c7-5c8a-4897-b458-bf5e43464d77"
      }
    }
  }
}
